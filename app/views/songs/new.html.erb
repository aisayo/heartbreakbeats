<%= form_for @song do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :artist %>
  <%= f.text_field :artist %>

  <% @vibes.each do |v| %>
  <%= v.name %>
  <%= f.label :level %>
  <%= f.text_field :level %>
  <% end %>

  <%= f.label :vibe %>
  <%= f.text_field :vibe %>

  <%= f.submit "add song" %>

<% end %>